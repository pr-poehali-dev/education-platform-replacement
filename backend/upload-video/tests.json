{
  "tests": [
    {
      "name": "OPTIONS request returns CORS headers",
      "method": "OPTIONS",
      "path": "/",
      "expectedStatus": 200,
      "expectedHeaders": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "name": "POST with invalid method returns 405",
      "method": "GET",
      "path": "/",
      "expectedStatus": 405
    }
  ]
}
